{
  "name": "express-sequelize-typescript-crud",
  "version": "1.0.0",
  "description": "CRUD operations with Express, Sequelize, and TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "start": "node dist/server.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all"
  },
  "keywords": ["express", "sequelize", "typescript", "crud", "mysql"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.37.7",
    "mysql2": "^3.14.3",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/body-parser": "^1.19.5",
    "typescript": "^5.4.2",
    "ts-node": "^10.9.2",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.2",
    "@types/sequelize": "^4.28.20"
  }
}

